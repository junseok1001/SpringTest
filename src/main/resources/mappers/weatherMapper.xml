<?xml version="1.0" encoding="utf-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sourjelly.springtest.thymeleaf.repository.WeatherRepository">
    <select id="selectWeatherTable"
    resultType="com.sourjelly.springtest.thymeleaf.domain.Weather">
        SELECT
            `id`
            , `date`
            , `weather`
            , `temperatures`
            , `precipitation`
            , `micro_dust`
            , `wind_speed`
        FROM
            `weatherhistory`
        ORDER BY
            `id`
    </select>
</mapper>